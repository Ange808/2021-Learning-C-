#include <iostream>
using namespace std;

int main()
{
    int f, s, t, z;

     cin >> f >> s >> t >> z;

     if (f > s, f > t, f > z) {
         cout << f << endl;
     }
     else if (f == s, t, z){
         cout << f << endl;
         if (s > t, z) {
             cout << s << endl;
         }
         }
         else if (s == t, z) {
             cout << s << endl;
             if (t > z) {
                 cout << t << endl;
             }
             }
             else if (t = z) {
                 cout << t << endl;
                 if (z > f, s, z) {
                     cout << z << endl;
                 }
                 }
    return 0;
}
